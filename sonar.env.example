# Environment Variables for SonarQube (EXAMPLE - DO NOT COMMIT WITH REAL TOKEN)
# Copy this file to sonar.env and add your actual token
# sonar.env is in .gitignore and safe for credentials

# SonarQube authentication token (get from http://localhost:9000)
export SONAR_TOKEN=your_token_here

# SonarQube server URL
export SONAR_HOST_URL=http://localhost:9000
